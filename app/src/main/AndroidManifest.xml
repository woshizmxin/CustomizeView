<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.twowater.customizeview">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:label="@string/title_activity_bezier_curve"
            android:name="com.twowater.customizeview.BeziercurvesExample.BezierCurveActivity"
            android:theme="@style/AppTheme.NoActionBar"></activity>
        <activity android:name="com.twowater.customizeview.QqmsgnotifyExample.QQMsgNotifyActivity"></activity>
        <activity android:name="com.twowater.customizeview.GuideviewExample.GuideViewActivity"></activity>
        <activity android:name=".wave.WaveActivity"></activity>
    </application>

</manifest>